    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/tampermonkey": "^5.0.4",
    "eslint": "^9.19.0",
    "rollup": "^4.1.4",
    "rollup-plugin-userscript-metablock": "^0.4.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
